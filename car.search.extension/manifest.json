{
    "manifest_version": 2,
    "name": "ford search",
    "description": "Quickly find vehicles on dealership sites.",
    "version": "1.0.0",
    "icons": {"128": "128.vehicle.search.png"},
    "browser_action": {
        "default_icon": "carSearch.logo.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": ["activeTab", "contextMenus"]
}